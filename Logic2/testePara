Algoritmo "testePara"

Var
   num, n, soma: inteiro

Inicio
   escreva("Quantos números de 0 à 9 serão digitados?")
   leia(num)

   para num de 1 ate num faca
      escreva("Digite um numero: ")
      leia(n)
      soma <- n + soma
      num <- num + 1
   fimpara

   escreval("SOMA = ", soma)
   

Fimalgoritmo
